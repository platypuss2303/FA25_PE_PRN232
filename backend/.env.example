# PostgreSQL Database Configuration
# Copy this file to appsettings.Development.json and fill in your actual values

# Local PostgreSQL Example:
# ConnectionStrings__Default=Host=localhost;Port=5432;Database=PostManagementDB;Username=postgres;Password=your_password

# Supabase Example:
# ConnectionStrings__Default=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Railway Example:
# ConnectionStrings__Default=postgresql://postgres:[PASSWORD]@[HOST]:[PORT]/railway

# Note: For appsettings.json, use this format:
# "ConnectionStrings": {
#   "Default": "Host=localhost;Port=5432;Database=PostManagementDB;Username=postgres;Password=your_password"
# }
